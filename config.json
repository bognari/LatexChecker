{
    "LATEXWHITELIST": {
        "lstinline": [],
        "Para": [],
        "FTNT": [],
        "qualityReq": [-1]
    },
    "PARTS": {
        "mychapter": 0,
        "mysection": 10,
        "prefsubtitle": 10,
        "ShortTitle": -5,
        "Chap": 0
    },
    "MODULES": {
        "Words": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "WordList": ["aber", "also", "du", "ich", "man", "todo", "tun", "Bild"],
            "CaseSensitive": false
        },
        "WordsRepetition": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListStartWith": "(?<Word>\\b(?!",
            "WordList": ["das", "die", "der", "the", "of", "dem", "des"],
            "ListEndWith": ")[a-z]{3,}+\\b)((.*?)(\\b\\k<Word>\\b)){3,}",
            "UseEscaping": false,
            "CaseSensitive": false
        },
        "Phrases": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "WordList": ["In diesem Kapitel", "In diesem Abschnitt"],
            "CaseSensitive": false
        },
        "SentenceStarts": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "WordList": ["Dass", "Aber", "Desto", "Da", "Weshalb", "Darum"],
            "OnlyAtStart": true,
            "Msg": "find: \"%1$s\""
        },
        "Numbers": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListStartWith": "(?<!\\d|,)([\\d]{1,1}|10|11|12)(?!(\\d|\\.|,))",
            "UseEscaping": false
        },
        "Desto": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListStartWith": "((?!je).)*(desto).*",
            "OnlyAtStart": true,
            "UseEscaping": false,
            "CaseSensitive": false
        },
        "Dass": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListStartWith": "(?<!,\\s{1,100})dass\\b",
            "UseEscaping": false,
            "CaseSensitive": false
        },
        "HeadlineCheck": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListEndWith": "(:\\s*)$",
            "UseEscaping": false,
            "Source": "headline"
        },
        "CaptionCheck": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListEndWith": "(:\\s*)$",
            "UseEscaping": false,
            "Source": "command",
            "SourceList": [
                "caption"
            ]
        },
        "ParagraphCheck": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListEndWith": "([\\.: ]\\s*)$",
            "UseEscaping": false,
            "Source": "headline",
            "HeadlineFrom": "60"
        },
        "BadSentenceLength": {
            "Class": "de.tubs.latexTool.modules.TextLength",
            "MinChars": 3,
            "MaxChars": 240,
            "MinWords": 3,
            "MaxWords": 80
        },
        "HeadlineLength": {
            "Class": "de.tubs.latexTool.modules.TextLength",
            "Source": "headline",
            "MinWords": 1,
            "MaxWords": 5
        },
        "CaptionLength": {
            "Class": "de.tubs.latexTool.modules.TextLength",
            "Source": "command",
            "SourceList": [
                "caption"
            ],
            "MinWords": 5
        },
        "SpellingText": {
            "Class": "de.tubs.latexTool.modules.LanguageToolChecker",
            "RulesXML": ["rules_de.xml", "rules_en.xml"]
        },
        "SpellingCommand": {
            "Class": "de.tubs.latexTool.modules.LanguageToolChecker",
            "Source": "command",
            "SourceList": [
                "caption"
            ]
        },
        "SpellingEnvironment": {
            "Class": "de.tubs.latexTool.modules.LanguageToolChecker",
            "Source": "environment",
            "SourceList": [
                "tabular"
            ]
        },
        "SpellingBullet": {
            "Class": "de.tubs.latexTool.modules.LanguageToolChecker",
            "Source": "bullet",
            "SourceList": [
                "itemize", "enumerate", "description"
            ]
        },
        "BadLatex": {
            "Class": "de.tubs.latexTool.modules.BadLatex",
            "Commands": ["textbf", "underline"],
            "Environments": []
        },
        "#BadLatex2": {
            "Class": "de.tubs.latexTool.modules.BadLatex",
            "Commands": ["textbf", "underline"]
        },
        "BadParagraphLength": {
            "Class": "de.tubs.latexTool.modules.ParagraphLength",
            "MinSentences": 4,
            "MaxSentences": 80
        },
        "LabelCheck": {
            "Class": "de.tubs.latexTool.modules.LabelCheck",
            "Environments": ["figure", "tabular"]
        },
        "HeadlineRegEx": {
            "Class": "de.tubs.latexTool.modules.HeadlineRegEx",
            "Environments": ["figure", "tabular"],
            "MaxWords": 5,
            "MinWords": 1,
            "MaxWordsCaption": 10,
            "MinWordsCaption": 1
        },
        "ItemCheck": {
            "Class": "de.tubs.latexTool.modules.ItemCheck",
            "MaxSentences": 80,
            "MinItems": 3,
            "MaxItems": 20,
            "Environments": [
                "itemize", "enumerate", "description"
            ]
        },
        "ChapterCheck": {
            "Class": "de.tubs.latexTool.modules.ChapterCheck",
            "MinSubChapters": 2,
            "MaxSubChapters": 8,
            "MaxChapterDepth": 6,
            "MinSentences": 2,
            "MaxSentences": 200
        },
        "EnvCheck": {
            "Class": "de.tubs.latexTool.modules.EnvTextCheck",
            "MinBetweenSentences": 2,
            "MinBeforeSentences": 2,
            "Environments": [
                "itemize", "enumerate", "description", "function", "data", "ui", "class", "entity", "component", "interface", "testcase"
            ]
        },
        "AbbreviationsCheck": {
            "Class": "de.tubs.latexTool.modules.AbbreviationsCheck",
            "BadSeparators": ["", " ", "\\ ", "~", "\\.", "\\qquad", "\\quad"]
        },
        "NoSpaceBeforeCite": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListStartWith": "(?<!~)",
            "WordList": ["\\\\cite"],
            "UseEscaping": false,
            "Source": "tex",
            "Msg": "find: \"%1$s\""
        },
        "NoCiteAtBegin": {
            "Class": "de.tubs.latexTool.modules.TextRegex",
            "ListStartWith": "^\\s*",
            "WordList": ["~?\\\\cite"],
            "UseEscaping": false,
            "Source": "latexText",
            "Msg": "find: \"%1$s\""
        },
        "NoAbbreviations": {
            "Class": "de.tubs.latexTool.modules.NoAbbreviations",
            "Separators": ["", " "]
        }
    }
}
